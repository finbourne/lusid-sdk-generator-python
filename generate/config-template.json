{
    "packageName": "${PACKAGE_NAME}",
    "projectName": "${PROJECT_NAME}",
    "packageVersion": "${PACKAGE_VERSION}",
    "generateSourceCodeOnly": "generateSourceCodeOnly",
    "library": "asyncio",
    "disallowAdditionalPropertiesIfNotPresent": false,
    "licenseInfo": "MIT",
    "datetimeFormat": "%Y-%m-%dT%H:%M:%S.%fZ",
    "isLusidPackage": true,
    "application": "${APPLICATION_NAME}",
    "gitUserId": "${GIT_USER_ID}",
    "gitRepoId": "${GIT_REPO_ID}",
    "additionalProperties": {
        "identifierNamingConvention": "snake_case"
    },
    "files": {
        "extensions/api_client_factory.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/api_client_factory.py"
        },
        "extensions/configuration_loaders.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/configuration_loaders.py"
        },
        "extensions/api_configuration.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/api_configuration.py"
        },
        "extensions/retry.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/retry.py"
        },
        "extensions/proxy_config.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/proxy_config.py"
        },
        "extensions/refreshing_token.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/refreshing_token.py"
        },
        "extensions/__init__.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/__init__.py"
        },
        "extensions/api_client.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/api_client.py"
        },
        "extensions/rest.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/rest.py"
        },
        "extensions/socket_keep_alive.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/socket_keep_alive.py"
        },
        "extensions/tcp_keep_alive_connector.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/tcp_keep_alive_connector.py"
        },
        "extensions/file_access_token.mustache": {
            "destinationFilename": "${PACKAGE_NAME}/extensions/file_access_token.py"
        }
    }
}